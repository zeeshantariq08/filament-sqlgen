{
    "name": "zeeshantariq/filament-sqlgen",
    "description": "A FilamentPHP package that generates SQL from natural language using Gemini AI.",
    "keywords": [
        "filament",
        "ai",
        "sqlgen",
        "gemini",
        "text-to-sql",
        "google-ai",
        "filamentphp",
        "natural-language-to-sql"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZeeshanTariq\\FilamentSqlGen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zeeshan Tariq",
            "email": "zeeshiq58@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/support": "^3.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZeeshanTariq\\FilamentSqlGen\\FilamentSqlGenServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
